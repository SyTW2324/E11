{
  "homepage": "https://wildle.github.io/wildle/",
  "name": "wildle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "npm run prepare && concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start",
    "prepare": "concurrently \"npm run prepare:server\" \"npm run prepare:client\"",
    "prepare:server": "cd server && npm install",
    "prepare:client": "cd client && npm install",
    "build": "npm run prepare && concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "test": "npm run prepare && concurrently \"npm run test:client\" \"npm run test:server\"",
    "test:client": "cd client && npm test",
    "test:server": "cd server && npm test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@auth0/auth0-react": "^2.2.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/jwt-decode": "^3.1.0",
    "@vercel/speed-insights": "^1.0.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lambdatest-cypress-cli": "^3.0.23",
    "mongodb": "^6.2.0",
    "puppeteer": "^21.6.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.6",
    "cypress": "^13.6.1",
    "gh-pages": "^6.1.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "postcss": "^8.4.32",
    "surge": "^0.23.1",
    "vercel": "^32.7.2"
  }
}
